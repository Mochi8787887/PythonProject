    "# 匯入OpenCV函式庫\n",
    "import cv2 as cv\n",
    "\n",
    "# 讀取圖片\n",
    "img = cv.imread('PEGA.jpg')\n",
    "\n",
    "# 賦予開啟的視窗名稱，開啟圖片\n",
    "cv.imshow('GGFP',img)\n",
    "\n",
    "# 設定 0 表示不主動關閉視窗\n",
    "cv.waitKey(0)"
   ]
